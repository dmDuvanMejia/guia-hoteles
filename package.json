{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Primer proyecto de Nodejs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "clean": "rimraf distri",
    "imagemin": "imagemin assets/img/* --out-dir distri/assets/img",
    "usemin": "usemin index.html -d distri --htmlmin -o distri/index.html && usemin precios.html -d distri --htmlmin -o distri/precios.html && usemin contacto.html -d distri --htmlmin -o distri/contacto.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "author": "Duvan Mejia",
  "license": "ISC",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
